<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="pers.lrq.mapper.GetImgDetailMapper">
    <select id="getSelectImgById" parameterType="integer" resultType="pers.lrq.domain.Selectimg">
        select tid,img from p_tourist where tid = #{tid}
    </select>

    <select id="getDeliciousImgById" parameterType="integer" resultType="pers.lrq.domain.Selectimg">
        select fid,img from p_food where fid = #{fid}
    </select>
</mapper>