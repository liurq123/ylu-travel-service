<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="pers.lrq.mapper.RegisterMapper">

    <insert id="SaveUser"   parameterType="pers.lrq.domain.User">
        INSERT INTO `yluUser`(`username`,`password`,`email`,`name`,`number`,`gender`,`birthdayDate`) VALUES
        (#{username},#{password},#{email},#{name},#{number},#{gender},#{birthdayDate});
    </insert>


</mapper>